{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "@id": "[Id](#Id)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Unique identifier in provider system"
    },
    "identifiers": {
      "@id": "[Id](#Id)entifiers",
      "@type": "xsd:string",
      "type": "string",
      "@contaner": "@set",
      "dldescription": "Array of unique URN identifiers"
    },
    "name": {
      "@id": "[Name](#Name)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Display name for the plot"
    },
    "activity": {
      "@id": "[Activity](#Activity)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Enumeration: the type of vegetation grown here: e.g. permanent  grassland"
    },
    "secondaryActivity": {
      "@id": "[SecondaryActivity](#SecondaryActivity)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Description of any secondary values relevant to the plot, i.e. fodder beet section on grazing plot"
    },
    "organic": {
      "@id": "[Organic](#Organic)",
      "@type": "xsd:boolean",
      "type": "boolean",
      "dldescription": "Indicates whether plot is organic"
    },
    "area": {
      "@id": "[Area](#Area)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Total paddock area expressed in m2"
    },
    "effectiveArea": {
      "@id": "[effectiveArea](#effectiveArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Plot effective area in m2"
    },
    "cultivableArea": {
      "@id": "[cultivableArea](#cultivableArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Effective area of the farm for cultivation (cropping) purposes"
    },
    "grazeableArea": {
      "@id": "[grazeableArea](#grazeableArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Plot grazeable area in m2"
    },
    "croppingArea": {
      "@id": "[croppingArea](#croppingArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": "Area planted in crop. In m2"
    },
    "drainageArea": {
      "@id": "[DrainageArea](#DrainageArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "irrigationArea": {
      "@id": "[IrrigationArea](#IrrigationArea)",
      "@type": "xsd:decimal",
      "type": "number",
      "dldescription": ""
    },
    "irrigationMethod": {
      "@id": "[IrrigationMethod](#IrrigationMethod)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": ""
    },
    "ownership": {
      "@id": "[Ownership](#Ownership)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Owned, Leased"
    },
    "topography": {
      "@id": "[Topography](#Topography)",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Values can be Flat, Rolling, Easy hill, Steep hill"
    },
    "validFrom": {
      "@id": "[PastureCoversFarm-ValidFrom](https://github.com/Datalinker-Org/Pasture-Management/blob/master/Pasture-Cover/PastureCoversFarm.md)",
      "@type": "xsd:dateTime",
      "type": "string",
      "dldescription": "ISO8601 date format 2018-07-19T23:00:08+00:00. Date started operating for specific plot type"
    },
    "validTo": {
      "@id": "[PastureCoversFarm-ValidTo](https://github.com/Datalinker-Org/Pasture-Management/blob/master/Pasture-Cover/PastureCoversFarm.md)",
      "@type": "xsd:dateTime",
      "type": "string",
      "dldescription": "ISO8601 date format 2018-07-19T23:00:08+00:00. Date ended(ceased) operating for specific plot type"
    }
  }
}
