{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@id": {
      "@id": "https://www.datalinker.org/context/MapsSite-@Id",
      "@type": "@id",
      "type": "string",
      "format": "uri",
      "dldescription": "Unique identifier in the source system"
    },
    "id": {
      "@id": "https://www.datalinker.org/context/MapsSite-Id",
      "@type": "@id",
      "type": "string",
      "dldescription": "Unique identifier in the source system"
    },
    "identifiers": {
      "@id": "https://www.datalinker.org/context/MapsSite-Identifiers",
      "@type": "xsd:string",
      "type": "string",
      "@contaner": "@set",
      "dldescription": "Array of unique URN identifiers"
    },
    "name": {
      "@id": "https://www.datalinker.org/context/MapsSite-Name",
      "@type": "xsd:string",
      "type": "string",
      "dldescription": "Display name for the site"
    },
    "featureCatalog": {
      "@id": "https://www.datalinker.org/context/MapsSite-FeatureCatalog",
      "@type": "@id",
      "type": "string",
      "format": "uri",
      "dltype": "https://www.datalinker.org/context/FeatureCatalogItem",
      "dldescription": "Url for getting list of catalog items with features(refer to https://www.datalinker.org/context/MapsFeatureCatalog) on a site"
    },
    "dateModified": {
      "@id": "http://schema.org/dateModified",
      "@type": "xsd:dateTime",
      "type": "string",
      "dldescription": "ISO8601 date format 2018-07-19T23:00:08+00:00. Date when details were last modified. Bubles up from Feature Catalog"
    }
  }
}